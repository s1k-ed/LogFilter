    "version": "0.2.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/bin/Debug/net8.0/Test.dll",
            "args": [
                "--file-log=GeneratedLog.txt",
                "--file-output=output.txt",
                "--time-start=01.01.2004",
                "--time-end=01.01.2024"
            ],
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "stopAtEntry": false
        }
    ]
}